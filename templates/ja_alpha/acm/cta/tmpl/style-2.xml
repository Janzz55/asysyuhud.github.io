<?xml version="1.0" encoding="utf-8"?>
<form>
	<title>Cta</title>
	<sampledata></sampledata>
	<fields name="parallax">
	   <fieldset name="basic" label="CTA_BASIC_LABEL" description="CTA_BASIC_DESC" multiple="0">
	      <field name="cta-logo" type="media" default="" label="CTA_LOGO_IMAGE_LABEL" description="CTA_LOGO_IMAGE_DESC" />
	      <field name="btn-link" type="text" default="" label="CTA_LINK_LABEL" description="CTA_LINK_DESC" />
	      <field name="btn-title" type="text" default="" label="CTA_TITLE_LABEL" description="CTA_TITLE_DESC" />
	  </fieldset>

	  <fieldset name="section-social" label="SOCIAL_SECTION_LABEL" description="SOCIAL_SECTION_DESC" multiple="1">
				<field name="font-icon" type="text" default="" label="SOCIAL_FONT_ICON_LABEL" description="SOCIAL_FONT_ICON_DESC" hint="fa fa-facebook"/>
				<field name="title" type="text" default="" size="40" label="SOCIAL_TITLE_LABEL" description="SOCIAL_TITLE_DESC" />
				<field name="link" type="text" default="" label="SOCIAL_LINK_LABEL" description="SOCIAL_LINK_DESC" rows="5" cols="40" required="1"/>
		</fieldset>
		
  </fields>
</form>



